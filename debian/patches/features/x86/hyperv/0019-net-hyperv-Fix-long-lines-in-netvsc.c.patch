Index: linux-3.2.46/drivers/net/hyperv/netvsc.c
===================================================================
--- linux-3.2.46.orig/drivers/net/hyperv/netvsc.c	2013-07-26 19:18:18.000000000 +0000
+++ linux-3.2.46/drivers/net/hyperv/netvsc.c	2013-07-26 19:18:20.000000000 +0000
@@ -230,9 +230,11 @@
 	net_device->recv_section_cnt = init_packet->msg.
 		v1_msg.send_recv_buf_complete.num_sections;
 
-	net_device->recv_section = kmemdup(init_packet->msg.v1_msg.send_recv_buf_complete.sections,
-					   net_device->recv_section_cnt * sizeof(struct nvsp_1_receive_buffer_section),
-					   GFP_KERNEL);
+	net_device->recv_section = kmemdup(
+		init_packet->msg.v1_msg.send_recv_buf_complete.sections,
+		net_device->recv_section_cnt *
+		sizeof(struct nvsp_1_receive_buffer_section),
+		GFP_KERNEL);
 	if (net_device->recv_section == NULL) {
 		ret = -EINVAL;
 		goto cleanup;
